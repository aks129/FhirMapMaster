{
  "functions": {
    "api/health.py": {
      "runtime": "python3.9"
    },
    "api/validate.py": {
      "runtime": "python3.9"
    },
    "api/transform.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}